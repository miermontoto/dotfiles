alias ll='ls -alF'
alias la='ls -a'
alias l='ls -CF'
alias lr='ls -R'
alias spicetify="~/spicetify-cli/spicetify"
alias please=sudo
alias bashrc="gedit ~/.bashrc"
alias zshrc="gedit ~/.zshrc"
alias aliases="gedit ~/.aliases"
alias alacrittyrc="gedit ~/.config/alacritty/alacritty.yml"
alias nfrc="nano ~/.config/neofetch/config.conf"
alias nf=neofetch
alias cls=clear
alias status="uptime ; nc -zv 192.168.0.2 22 -w 1 ; nc -zv ubuntu.com 80 -w 1 ; nc -zv 192.168.0.10 22 -w 1 ; nc -zv 192.168.0.88 22 -w 1 ; nc -zv 192.168.0.17 22 -w 1"
alias nethogs="sudo nethogs"
alias "cd.."="cd .."
alias remove="sudo apt remove -y"
alias install="sudo apt install -y"
alias update="sudo apt update -y"
