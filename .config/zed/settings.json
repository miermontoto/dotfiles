  "features": {
    "edit_prediction_provider": "zed"
  },
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "project_panel": {
    "dock": "right"
  },
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "ui_font_family": "TX-02",
  "buffer_font_size": 18,
  "buffer_font_family": "TX-02",
  "buffer_font_weight": 500,
  "buffer_line_height": "comfortable",
  "terminal": {"font_family": "BerkeleyMono Nerd Font"},
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Light"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "format_on_save": "on",
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "extend_comment_on_newline": false,
  "wrap_guides": [80],
  "show_wrap_guides": true,
  "languages": {
    "PHP": {
      "language_servers": ["intelephense", "!phpactor"],
      "prettier": {
        "allowed": false
      }
    }
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  }
}
