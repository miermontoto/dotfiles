alias purge="sudo apt -y purge"
alias list="sudo apt list"
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'
alias reboot="sudo reboot"
alias onedrive="~/launchOneDrive.sh"
alias ssh="TERM=xterm-256color ssh"
alias server="TERM=xterm-256color ssh server@192.168.0.88"
alias pi="TERM=xterm-256color ssh pi@192.168.0.2"
alias dirbuster="source /opt/dirbuster/DirBuster-1.0-RC1.sh"
alias htb="sudo openvpn ~/Downloads/*.ovpn"
alias nmap="sudo nmap"
alias nano="sudo nano"
alias hc="headsetcontrol"
alias %%="%1"
